@page
@model SeguimientoAcademico.App.Presentacion.Pages.EliminarModel
@{
}


<h1>Eliminar</h1>

<form method="post">

    
    <div class="form-group">
    <label asp-for="estudiante.Id"></label>
    <input asp-for="estudiante.Id" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="estudiante.Nombre"></label>
    <input asp-for="estudiante.Nombre" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="estudiante.Apellido"></label>
    <input asp-for="estudiante.Apellido" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="estudiante.Telefono"></label>
    <input asp-for="estudiante.Telefono" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="estudiante.Genero"></label>
    <input asp-for="estudiante.Genero" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="estudiante.Ciudad"></label>
    <input asp-for="estudiante.Ciudad" class="form-control" readonly>
    </div>



    <button type="submit" class="btn btn-primary" >Eliminar</button>
    <button href="/Estudiantes/Index" ></button>

    

</form>

