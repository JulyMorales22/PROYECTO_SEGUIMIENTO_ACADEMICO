@page
@model SeguimientoAcademico.App.Presentacion.Pages.EliminarMaestrosModel
@{
}

<h1>Eliminar</h1>

<form method="post">

    
    <div class="form-group">
    <label asp-for="maestro.Id"></label>
    <input asp-for="maestro.Id" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="maestro.Nombre"></label>
    <input asp-for="maestro.Nombre" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="maestro.Apellido"></label>
    <input asp-for="maestro.Apellido" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="maestro.Telefono"></label>
    <input asp-for="maestro.Telefono" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="maestro.Genero"></label>
    <input asp-for="maestro.Genero" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="maestro.LicenciaProfesional"></label>
    <input asp-for="maestro.LicenciaProfesional" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="maestro.Especialidad"></label>
    <input asp-for="maestro.Especialidad" class="form-control" readonly>
    </div>



    <button type="submit" class="btn btn-primary" >Eliminar</button>
    <button href="/Maestros/IndexMaestros" ></button>
    <a href="/Maestros/IndexMaestros"><button type="button" class="btn btn-secundary">Regresar</button></a>

    

</form>

