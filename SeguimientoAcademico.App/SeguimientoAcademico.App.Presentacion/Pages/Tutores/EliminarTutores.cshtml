@page
@using SeguimientoAcademico.App.Dominio
@model SeguimientoAcademico.App.Presentacion.Pages.EliminarTutoresModel
@{
}

<h1>Eliminar</h1>

<form method="post">

    
    <div class="form-group">
    <label asp-for="tutor.Id"></label>
    <input asp-for="tutor.Id" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="tutor.Nombre"></label>
    <input asp-for="tutor.Nombre" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="tutor.Apellido"></label>
    <input asp-for="tutor.Apellido" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="tutor.Telefono"></label>
    <input asp-for="tutor.Telefono" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="tutor.Genero"></label>
    <input asp-for="tutor.Genero" class="form-control" readonly>
    </div>

    <div class="form-group">
    <label asp-for="tutor.MateriaAsignada"></label>
    <input asp-for="tutor.MateriaAsignada" class="form-control" readonly>
    </div>

   



    <button type="submit" class="btn btn-primary" >Eliminar</button>
    <a href="/Tutores/IndexTutores"><button type="button" class="btn btn-secundary">Regresar</button></a>

    

</form>